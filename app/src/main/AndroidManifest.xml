<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abdemanaaf.nulircapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".NULIRCApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuickLinks"
            android:label="@string/title_activity_quick_links"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginPage"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".SignUpPage"
            android:label="@string/title_activity_sign_up" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".AdminLogin"
            android:label="@string/title_activity_admin_login" />
        <activity
            android:name=".AboutActivity"
            android:label="@string/title_activity_about"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EResourcesActivity"
            android:label="@string/title_activity_eresources"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>